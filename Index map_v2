    "nu1 = 203e9\n",
    "nu2 = 398e9\n",
    "ln_nu = np.log(nu2/nu1)\n",
    "\n",
    "nsig =3.0  # 3–5 típico\n",
    "mask = (\n",
    "    np.isfinite(I1) & np.isfinite(I2) &\n",
    "    (I1 > nsig*rms3) & (I2 > nsig*rms4) &\n",
    "    (I1 > 0) & (I2 > 0)\n",
    ")\n",
    "\n",
    "alpha = np.full(I1.shape, np.nan, dtype=float)\n",
    "alpha[mask] = np.log(I2[mask]/I1[mask]) / ln_nu"
   ]
